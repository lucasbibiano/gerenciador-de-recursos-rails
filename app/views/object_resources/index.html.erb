<h1>Listing object_resources</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Tombamento</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @object_resources.each do |object_resource| %>
  <tr>
    <td><%= object_resource.name %></td>
    <td><%= object_resource.description %></td>
    <td><%= object_resource.tombamento %></td>
    <td><%= link_to 'Show', object_resource %></td>
    <td><%= link_to 'Edit', edit_object_resource_path(object_resource) %></td>
    <td><%= link_to 'Destroy', object_resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

